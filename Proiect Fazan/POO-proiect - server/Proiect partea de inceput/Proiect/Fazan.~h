#ifndef FazanH
#define FazanH


class FazanPlayer{
protected:
        String fazan;
        int scor;
public:
        FazanPlayer();
        FazanPlayer(String f, int s);
        ~FazanPlayer();
        void setFazan(String f);
        String getFazan();
        void setScor(int s);
        int getScor();
};


#endif
